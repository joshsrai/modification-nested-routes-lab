<%= form_for @song  do |f| %>
  <%= artist_select(@song) %>
  <br> 
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.submit %>
<% end %>

